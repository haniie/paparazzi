<!DOCTYPE module SYSTEM "../module.dtd">
	<module name="gy86_imu_baro" dir="autmav">
		<doc>
			<description>
		     auxilary barometer module for AUTMAV apogee boards.	
			 driver for using MPU6050 + MS5611 chips on I2C bus. 
   			</description>
			<define name="AUX_BARO_LED" value="none"/>		
		</doc>
  
 		<header>
 	  		<file name="gy86_imu_baro.h"/>
 	 	</header>
  
 		<init fun="gy86_mpu_init()"/>
		<init fun="gy86_baro_init()"/>

  		<periodic fun="gy86_baro_periodic()" freq="50" autorun="LOCK"/> 
  		<periodic fun="gy86_mpu_periodic()" freq="50" autorun="LOCK"/> 
  
  		<event fun="gy86_baro_event()"/> 
  		<event fun="gy86_mpu_event()"/> 

  		<makefile>
			<raw>
 				ap.srcs += peripherals/ms5611_i2c.c
				ap.srcs += peripherals/ms5611.c
			</raw>   
			<file name="gy86_imu_baro.c"/>
		</makefile>
 
 </module>
