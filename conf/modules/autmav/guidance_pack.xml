<!DOCTYPE module SYSTEM "../module.dtd">

<module name="guidance_pack" dir="autmav">
  <doc>
    <description> Guidance Pack </description>
  </doc>
  <header>
    <file name="guidance_pack.h"/>
  </header>
  <init fun="guidance_pack_init()"/>
  <periodic fun="guidance_pack_periodic()" freq="60."/>
  <event fun="guidance_pack_event()"/>
  <makefile target="ap">
    <file name="guidance_pack.c"/>
    <file name="sbus_command.c"/>
    <define name="USE_UART6"/>
    <define name="SBUS_COMMAND_PORT" value="uart6"/>
    <define name="UART6_BAUD" value="100000"/>
    <define name="UART6_STOP" value="USTOP_2"/>
    <define name="UART6_PARITY" value="UPARITY_EVEN"/>

  </makefile>
</module>


