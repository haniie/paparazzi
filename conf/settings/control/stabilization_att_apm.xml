<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings target="ap|nps">
  <dl_settings>

    <dl_settings NAME="Att Loop">
      <dl_setting var="stabilization_gains.p.x" min="0" step="1" max="8000"   module="stabilization/stabilization_attitude" shortname="p gain phi" param="STABILIZATION_ATTITUDE_PHI_PGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.i.x" min="0" step="1" max="800"    module="stabilization/stabilization_attitude" shortname="i gain phi" param="STABILIZATION_ATTITUDE_PHI_IGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.d.x" min="0" step="1" max="4000"   module="stabilization/stabilization_attitude" shortname="d gain phi" param="STABILIZATION_ATTITUDE_PHI_DGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.rates_p.x" min="0" step="1" max="1000"   module="stabilization/stabilization_attitude" shortname="p rate phi" param="STABILIZATION_ATTITUDE_PHI_RATE_PGAIN" persistent="true"/>

      <dl_setting var="stabilization_gains.p.y" min="0" step="1" max="8000"   module="stabilization/stabilization_attitude" shortname="p gain theta" param="STABILIZATION_ATTITUDE_THETA_PGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.i.y" min="0"  step="1" max="800"    module="stabilization/stabilization_attitude" shortname="i gain theta" param="STABILIZATION_ATTITUDE_THETA_IGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.d.y" min="0" step="1" max="4000"   module="stabilization/stabilization_attitude" shortname="d gain theta" param="STABILIZATION_ATTITUDE_THETA_DGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.rates_p.y" min="0" step="1" max="1000"   module="stabilization/stabilization_attitude" shortname="p rate theta" param="STABILIZATION_ATTITUDE_THETA_RATE_PGAIN" persistent="true"/>

      <dl_setting var="stabilization_gains.p.z" min="0" step="1" max="8000"   module="stabilization/stabilization_attitude" shortname="pgain psi" param="STABILIZATION_ATTITUDE_PSI_PGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.i.z" min="0"  step="1" max="800"    module="stabilization/stabilization_attitude" shortname="igain psi" param="STABILIZATION_ATTITUDE_PSI_IGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.d.z" min="0" step="1" max="4000"   module="stabilization/stabilization_attitude" shortname="dgain psi" param="STABILIZATION_ATTITUDE_PSI_DGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.rates_p.z" min="0" step="1" max="1000"   module="stabilization/stabilization_attitude" shortname="p rate psi" param="STABILIZATION_ATTITUDE_PSI_RATE_PGAIN" persistent="true"/>


   
    </dl_settings>

  </dl_settings>
</settings>
