<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings target="ap|nps">
  <dl_settings>

    <dl_settings NAME="Att Loop">
      <dl_setting var="lambda_roll" min="0" step="0.5" max="100"   module="stabilization/stabilization_attitude" shortname="lambda_roll" param="STABILIZATION_ATTITUDE_LAMBDA_ROLL" persistent="true"/>
		<dl_setting var="mio_roll" min="0" step="0.5" max="50"   module="stabilization/stabilization_attitude" shortname="mu_roll" param="STABILIZATION_ATTITUDE_MIO_ROLL" persistent="true"/>
		<dl_setting var="roll_gain" min="0" step="1000" max="500000"   module="stabilization/stabilization_attitude" shortname="roll_gain" param="STABILIZATION_ATTITUDE_ROLL_GAIN" persistent="true"/>
		<dl_setting var="beta_roll" min="0" step="1" max="100"   module="stabilization/stabilization_attitude" shortname="beta_roll" param="STABILIZATION_ATTITUDE_BETA_ROLL" persistent="true"/>
		<dl_setting var="alpha_roll" min="0" step="1" max="100"   module="stabilization/stabilization_attitude" shortname="alpha_roll" param="STABILIZATION_ATTITUDE_ALPHA_ROLL" persistent="true"/>
		
		<dl_setting var="lambda_pitch" min="0" step="0.5" max="100"   module="stabilization/stabilization_attitude" shortname="lambda_pitch" param="STABILIZATION_ATTITUDE_LAMBDA_PITCH" persistent="true"/>
		<dl_setting var="mio_pitch" min="0" step="0.5" max="50"   module="stabilization/stabilization_attitude" shortname="mu_pitch" param="STABILIZATION_ATTITUDE_MIO_PITCH" persistent="true"/>
		<dl_setting var="pitch_gain" min="0" step="1000" max="500000"   module="stabilization/stabilization_attitude" shortname="pitch_gain" param="STABILIZATION_ATTITUDE_PITCH_GAIN" persistent="true"/>
		<dl_setting var="beta_pitch" min="0" step="1" max="100"   module="stabilization/stabilization_attitude" shortname="beta_pitch" param="STABILIZATION_ATTITUDE_BETA_PITCH" persistent="true"/>
		<dl_setting var="alpha_pitch" min="0" step="1" max="100"   module="stabilization/stabilization_attitude" shortname="alpha_pitch" param="STABILIZATION_ATTITUDE_ALPHA_PITCH" persistent="true"/>
		
		<dl_setting var="lambda_yaw" min="0" step="0.5" max="100"   module="stabilization/stabilization_attitude" shortname="lambda_yaw" param="STABILIZATION_ATTITUDE_LAMBDA_YAW" persistent="true"/>
		<dl_setting var="mio_yaw" min="0" step="0.5" max="50"   module="stabilization/stabilization_attitude" shortname="mu_yaw" param="STABILIZATION_ATTITUDE_MIO_YAW" persistent="true"/>
		<dl_setting var="yaw_gain" min="0" step="1000" max="500000"   module="stabilization/stabilization_attitude" shortname="yaw_gain" param="STABILIZATION_ATTITUDE_YAW_GAIN" persistent="true"/>
		<dl_setting var="beta_yaw" min="0" step="1" max="100"   module="stabilization/stabilization_attitude" shortname="beta_yaw" param="STABILIZATION_ATTITUDE_BETA_YAW" persistent="true"/>
		<dl_setting var="alpha_yaw" min="0" step="1" max="100"   module="stabilization/stabilization_attitude" shortname="alpha_yaw" param="STABILIZATION_ATTITUDE_ALPHA_YAW" persistent="true"/>
		
		
		 <dl_setting var="e_center" min="0" step="0.005" max="1"   module="stabilization/stabilization_attitude" shortname="e center" param="STABILIZATION_ATTITUDE_E_center" persistent="true"/>
		
      <dl_setting var="stabilization_gains.i.z" min="0"  step="1" max="800"    module="stabilization/stabilization_attitude" shortname="igain psi" param="STABILIZATION_ATTITUDE_PSI_IGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.d.z" min="0" step="1" max="4000"   module="stabilization/stabilization_attitude" shortname="dgain psi" param="STABILIZATION_ATTITUDE_PSI_DGAIN" persistent="true"/>
      <dl_setting var="stabilization_gains.rates_p.z" min="0" step="1" max="1000"   module="stabilization/stabilization_attitude" shortname="p rate psi" param="STABILIZATION_ATTITUDE_PSI_RATE_PGAIN" persistent="true"/>
     

      <dl_setting var="guidance_v_nominal_throttle" min="0" step=".05" max="1"   module="guidance/guidance_v" shortname="nominal throttler" param="GUIDANCE_V_NOMINAL_HOVER_THROTTLE" persistent="true"/>

 <dl_setting var="guidance_h.gains.p" min="0" step=".05" max="100"   module="guidance/guidance_h" shortname="gui kp" param="GUIDANCE_H_PGAIN" persistent="true"/>
 <dl_setting var="guidance_h.gains.i" min="0" step=".05" max="100"   module="guidance/guidance_h" shortname="gui ki" param="GUIDANCE_H_IGAIN" persistent="true"/>
 <dl_setting var="guidance_h.gains.d" min="0" step=".05" max="100"   module="guidance/guidance_h" shortname="gui kd" param="GUIDANCE_H_DGAIN" persistent="true"/>
 <dl_setting var="guidance_h.gains.a" min="0" step=".05" max="100"   module="guidance/guidance_h" shortname="gui ka" param="GUIDANCE_H_AGAIN" persistent="true"/>
    </dl_settings>
  </dl_settings>
</settings>
